from selenium import webdriver
import time
driver = webdriver.Chrome('C:/chromedriver')  # Optional argument, if not specified will search path.
driver.get('https://cloud.memsource.com/web/job2/list')
time.sleep(1)

username = driver.find_element_by_name("username")
password = driver.find_element_by_name("password")
username.send_keys("xxxxxxxxxxx@gmail.com")
password.send_keys("xxxxxxxxxxx")
giris_yap = driver.find_element_by_xpath("/html/body/div[2]/div/form/div[4]/input")
giris_yap.click()
# siteye giriş yapıldı
time.sleep(0.5)
i=5
while(int(i)==5):

    issec= driver.find_element_by_xpath("//*[@id=""'jobSelectionToggle'""]")
    issec.click()
    accept = driver.find_element_by_xpath("//*[@id=""'acceptJobs'""]")
    accept.click()
    driver.refresh()


